#include <iostream>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin >> n;

    int wysokosci[1000000];
    int maks = -1000000000;

    for (int i = 0; i < n; i++) {
        cin >> wysokosci[i];
        if (wysokosci[i] > maks)
            maks = wysokosci[i];
    }

    cout << maks << "\n";
    return 0;
}